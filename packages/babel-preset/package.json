{
  "name": "@younho9/babel-preset",
  "version": "0.3.5",
  "description": "Sharable set of Babel plugins and config options",
  "keywords": [
    "babel",
    "babelpreset",
    "babel-preset"
  ],
  "homepage": "https://github.com/younho9/lib#readme",
  "bugs": {
    "url": "https://github.com/younho9/lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younho9/lib.git",
    "directory": "packages/babel-preset"
  },
  "license": "MIT",
  "author": "Younho Choo <yo+github@younho9.com>",
  "type": "commonjs",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn clean:dist",
    "build": "concurrently yarn:build:*",
    "build:cjs": "babel src -d dist --source-maps",
    "clean": "concurrently yarn:clean:*",
    "clean:dist": "rimraf dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "babel-plugin-add-module-exports": "1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@types/babel__helper-plugin-utils": "7.10.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
