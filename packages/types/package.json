{
  "name": "@younho9/types",
  "version": "0.10.1",
  "description": "A set of utility types for TypeScript",
  "keywords": [
    "typescript",
    "types"
  ],
  "homepage": "https://github.com/younho9/lib#readme",
  "bugs": {
    "url": "https://github.com/younho9/lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younho9/lib.git",
    "directory": "packages/types"
  },
  "license": "MIT",
  "author": "Younho Choo <yo+github@younho9.com>",
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn clean:dist",
    "build": "concurrently yarn:build:*",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist --source-maps",
    "build:types": "tsc src/*.ts --emitDeclarationOnly --declaration --declarationDir dist/types",
    "clean": "concurrently yarn:clean:*",
    "clean:dist": "rimraf dist",
    "lint": "concurrently yarn:lint:*",
    "lint:src": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "test": "concurrently yarn:test:*",
    "test:src": "jest --config ../../jest.config.cjs -- packages/types",
    "test:types": "tsd"
  },
  "dependencies": {
    "ts-indexify": "1.0.0"
  },
  "devDependencies": {
    "@younho9/eslint-plugin": "0.4.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsd": {
    "directory": "__tests__"
  }
}
